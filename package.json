{
    "name": "@harvard-lts/mirador-citation-plugin",
    "license": "Apache-2.0",
    "repository": "https://github.com/harvard-lts/mirador-custom-text-search-plugin.git",
    "keywords": [
        "react-component"
    ],
    "version": "0.0.10",
    "description": "A Mirador 3 plugin for displaying Harvard-specific citations.",
    "module": "dist/es/index.js",
    "files": [
        "dist"
    ],
    "scripts": {
        "clean": "rm -rf ./dist && rm -rf ./demo/dist",
        "prebuild": "npm run clean",
        "prepublishOnly": "npm run build",
        "build": "run-p build:es",
        "build:es": "rollup -c",
        "serve": "vite",
        "test": "vitest run",
        "test:coverage": "vitest run --coverage",
        "test:watch": "vitest"
    },
    "devDependencies": {
        "@babel/cli": "^7.26.4",
        "@babel/core": "^7.26.0",
        "@babel/preset-env": "^7.21.4",
        "@babel/preset-react": "^7.18.6",
        "@material-ui/core": "^4.12.4",
        "@material-ui/icons": "^4.11.2",
        "@rollup/plugin-babel": "^6.0.3",
        "@testing-library/jest-dom": "^6.1.5",
        "@testing-library/react": "^12.0.0",
        "mirador": "^3.4.3",
        "npm-run-all": "^4.1.5",
        "rollup": "^3.29.5",
        "whatwg-fetch": "^3.6.2"
    },
    "dependencies": {
        "@vitejs/plugin-react": "^5.1.2",
        "@vitest/coverage-v8": "^4.0.17",
        "@vitest/ui": "^4.0.17",
        "jquery": "^3.7.0",
        "jsdom": "^27.4.0",
        "prop-types": "^15.8.1",
        "react": "^16.14.0",
        "react-dom": "^16.14.0",
        "vite": "^7.3.1",
        "vitest": "^4.0.17"
    }
}
